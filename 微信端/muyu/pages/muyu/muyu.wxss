page{
    background-color: #1b1b1b;
    height: 100%;
}
.container{
    width:100%;
    height:100vh;
    z-index: 3;
    position: absolute;
    
}
.muyuicon{
    width: 435rpx; 
    height: 435rpx; 
    display: inline-block; 
    box-sizing: border-box; 
    position: absolute;
    left: 158rpx; 
    top: 500rpx; 
    z-index: 1;
    margin: auto;
}
.ripple {
    background-color: rgba(182, 182, 182, 0.6);
    border-radius: 100%;
    height:20px;
    width:20px;
    margin-top: -90px;
    position: absolute;
    -webkit-transform: scale(0);
}
@-webkit-keyframes ripple {
    100% {
    -webkit-transform: scale(18);
    transform: scale(18);
    background-color: transparent;
    }
    
}